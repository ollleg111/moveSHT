<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <declare-styleable name="FontView" >
         
        <attr name="fontName" format="integer"> 
            <enum name="ArimoBold" value="1" /> 
            <enum name="ArimoBoldItalic" value="2" /> 
            <enum name="ArimoItalic" value="3" /> 
            <enum name="BebasNeueBold" value="4" /> 
            <enum name="BebasNeueBook" value="5" /> 
            <enum name="BebasNeueLight" value="6" /> 
            <enum name="BebasNeueRegular" value="7" /> 
            <enum name="BebasNeueThin" value="8" /> 
            <enum name="ArimoRegular" value="1000" /> 
        </attr>
         <attr name="fontId" format="integer" /> 
    </declare-styleable>

    <style name="BlackEditText" parent="Base.Widget.AppCompat.EditText">
        <item name="android:textSize">@dimen/font_4</item>
        <item name="android:gravity">center</item>
        <item name="android:background">@drawable/rounded_text</item>
        <item name="android:layout_width">@dimen/size_text_one</item>
        <item name="android:layout_height">@dimen/margin_medium</item>
        <item name="android:layout_marginLeft">@dimen/margin_big</item>
        <item name="android:layout_marginRight">@dimen/margin_big</item>
        <item name="android:imeOptions">actionNext</item>
        <item name="android:singleLine">true</item>
        <item name="android:maxLines">1</item>
    </style>

    <style name="BtnCalc" parent="Base.Widget.AppCompat.Button">
        <item name="android:layout_width">@dimen/size_9</item>
        <item name="android:layout_height">@dimen/size_9</item>
        <item name="android:background">@drawable/button_calc</item>
        <item name="android:textColor">@color/colorWhite</item>
        <item name="android:gravity">center</item>
        <item name="android:clickable">true</item>
        <item name="android:focusable">true</item>
        <item name="android:focusableInTouchMode">true</item>
        <item name="android:textSize">@dimen/font_12</item>
        <item name="android:layout_margin">6dp</item>
    </style>

